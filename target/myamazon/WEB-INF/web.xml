<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
         http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <servlet-name>HomeServlet</servlet-name>
    <servlet-class>myservlet.HomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HomeServlet</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>CartServlet</servlet-name>
    <servlet-class>myservlet.CartServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CartServlet</servlet-name>
    <url-pattern>/cart</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>CartShowServlet</servlet-name>
    <servlet-class>myservlet.CartShowServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CartShowServlet</servlet-name>
    <url-pattern>/cartshow</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>AddCartServlet</servlet-name>
    <servlet-class>myservlet.AddCartServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddCartServlet</servlet-name>
    <url-pattern>/addcart</url-pattern>
  </servlet-mapping>






</web-app>
